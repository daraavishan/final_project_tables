<h1>
  Add a new user top artist image
</h1>


<hr>

<!-- Form user_top_artist_image start -->
<form action="/create_user_top_artist_image" method="post">
<!-- Form user_top_artist_image end -->

  <!-- Label and input for user_id start -->
  <div class="form-group">
    <label for="user_id">
      User
    </label>

    <!-- Input for user_id start -->
    <input type="text" id="user_id" name="user_id" class="form-control">
    <!-- Input for user_id end -->
  </div>
  <!-- Label and input for user_id end -->

  <!-- Label and input for image_url start -->
  <div class="form-group">
    <label for="image_url">
      Image url
    </label>

    <!-- Input for image_url start -->
    <input type="text" id="image_url" name="image_url" class="form-control">
    <!-- Input for image_url end -->
  </div>
  <!-- Label and input for image_url end -->

  <button class="btn btn-block btn-outline-secondary">
    Create user top artist image
  </button>
</form>

<hr>

<a href="/user_top_artist_images">
  Go back
</a>
